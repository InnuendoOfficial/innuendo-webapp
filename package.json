{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "start": "serve -s dist -l tcp://0.0.0.0:${PORT:-8080} -n",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "jest tests/jest",
    "test:e2e": "npx cypress open-ct",
    "ci:e2e": "npx cypress run-ct",
    "ci:unit": "jest tests/jest --coverage --ci",
    "build-typedoc": "npx typedoc --out typedoc ./src/main.ts --tsconfig ./tsconfig.json",
    "lint": "eslint . --ext .ts,.js"
  },
  "dependencies": {
    "@cypress/vite-dev-server": "^2.0.2",
    "@cypress/vue": "^3.1.1",
    "@quasar/extras": "^1.13.5",
    "cypress": "^9.6.0",
    "quasar": "^2.6.6",
    "serve": "^13.0.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@cypress/code-coverage": "^3.9.12",
    "@quasar/vite-plugin": "^1.0.9",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/vue3": "^6.4.22",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/vue3-jest": "^27.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.0.1",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "sass": "^1.32.0",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  }
}
